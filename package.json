{
  "name": "vue-markit",
  "version": "0.0.0",
  "description": "vue WYSIWYG markdown editor",
  "author": "nkjmsss",
  "private": false,
  "scripts": {
    "lint": "cross-env NODE_ENV=\"production\" eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "cross-env NODE_ENV=\"production\" eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint:dev": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix:dev": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "cross-env NODE_ENV=\"production\" npm run lint",
    "dev": "nuxt",
    "build": "cross-env NODE_ENV=\"production\" nuxt build",
    "start": "cross-env NODE_ENV=\"production\" nuxt start",
    "generate": "cross-env NODE_ENV=\"production\" nuxt generate"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.18.9",
    "nuxt": "^2.8.1",
    "prettier": "^1.16.4",
    "ts-node": "^8.3.0",
    "vue-property-decorator": "^8.2.1"
  },
  "dependencies": {
    "emotion": "^10.0.14",
    "lodash": "^4.17.11",
    "ts-keycode-enum": "^1.0.6"
  }
}
